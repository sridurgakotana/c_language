#include<stdio.h>
int main()
{
	int A[10][10],B[10][10],C[10][10],r1,c1,r2,c2,i,j,k;
	printf("Enter A matrix rows & columns sizes");
	scanf("%d%d",&r1,&c1);
	printf("Enter B matrix rows & columns sizes");
	scanf("%d%d",&r2,&c2);
	if(r1=c2)
	{
		printf("matrix multiplication  is not possible");
		exit(1);
    }
    printf("Enter A matrix values");
    for(i=0;i<r1;i++)
    {
    	for(j=0;j<c1;j++)
    	{
    		scanf("%d",&A[i][j]);
		}
	}
	printf("Enter B matrix values");
	for(i=0;i<r2;i++)
	{
		for(j=0;j<c2;j++)
		{
			scanf("%d",&B[i][j]);
		}
	}
	for(i=0;i<r1;i++)
	{
		for(j=0;j<c2;j++)
		C[i][j]=0;
		for(k=0;k<r2;k++)
		{
			C[i][j]=C[i][i]+A[i][j]*B[i][j];
		}
		
	}
	printf("Result matrix is \n");
	for(i=0;i<r1;i++)
	{
		for(j=0;j<c1;j++)
		{
			printf("%d",C[i][j]);
		}
	}
	for(i=0;i<r2;i++)
	{
		for(j=0;j<c2;j++)
		{
			printf("%d",C[i][j]);
		}
	}
	return 0;	
}

output:
Enter A matrix rows & columns sizes2
2
Enter B matrix rows & columns sizes2
2
matrix multiplication  is not possible

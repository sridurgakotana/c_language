#include<stdio.h>
#include<stdlib.h>
int main()
{
	FILE *fp;
	char ch;
	fp=fopen("HELLO.C","r");
	if(fp==NULL)
	{
		printf("File doesn't exist");
		exit(0);
	}
	ch=fgetc(fp);
	while(ch!=EOF)
	{
		printf("%c",ch);
		ch=fgetc(fp);
	}
	fclose(fp);
	return 0;
}
output:#include<stdio.h>
int main()
{

        printf("HELLO ALL");
        return 0;
}

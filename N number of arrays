#include<stdio.h>
int main()
{
	int a[100],i,n;
	printf("Enter array size");
	scanf("%d",&n);
	printf("Enter array value");
	for(i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
    }
    printf("\n Display array values");
    for(i=0;i<n;i++)
    {
    	printf("\n %d%d%u",a[i],&a[i]);
	}
	return 0;
}
output:Enter array size
3
Enter array value
2
4
6

 Display array values
 264872162424571136
 464872202424571136
 664872242424571136

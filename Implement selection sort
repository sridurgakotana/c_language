#include<stdio.h>
int main()
{
	int a[100],n,i,j,temp;
	printf("Enter array size");
	scanf("%d",&n);
	printf("Enter array values");
	for(i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
	}
	for(i=0;i<n;i++)
	{
		if(a[i]>a[j])
		{
			temp=a[i];
			a[i]=a[j];
			a[j]=temp;
		}
    }
    printf("\n sorted array is :");
    for(i=0;i<n;i++)
    {
    	printf("%d",a[i]);
	}
	return 0;
}
output:Enter array size
2
Enter array values
2
1

 sorted array is :12

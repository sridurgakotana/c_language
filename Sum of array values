	#include<stdio.h>
	int main()
	{
		int a[100],n,i,sum;
		printf("Enter array size");
		scanf("%d",&n);
		printf("\n Enter array values");
		for(i=0;i<n;i++)
		{
			scanf("%d",&a[i]);
		}
		for(i=0;i<n;i++)
		{
			sum=(sum+a[i]);
		}
		printf("sum of array valus=%d",sum);
		return 0;
	}
output:Enter array size
3

 Enter array values4
5
6
sum of array valus=16
